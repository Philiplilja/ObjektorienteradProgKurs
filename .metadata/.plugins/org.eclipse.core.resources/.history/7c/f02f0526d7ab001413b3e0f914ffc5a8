package se.his.a14phili;

public class HighscoreItem {
	private static final long serialVersionUID = 1L;
	
	private String name;
	private int score;
	private int value;
	
	public HighscoreItem(String name)
	{
		this.setName(name);
		this.setValue(value);
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getScore() {
		return score;
	}

	public void setScore(int score) {
		this.score = score;
	}

	public int getValue() {
		return value;
	}

	public void setValue(int value) {
		this.value = value;
	}
}
